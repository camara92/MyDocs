{% extends 'base.html.twig' %}

{% block title %}Réception - MyDocs {% endblock %}

{% block body %}
<style>
    .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
    .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
</style>

<div class="example-wrapper">
    {# <h1>Contrôle à la réception </h1>
        {{form(form)}} #}


        <div class="container bg-forms gc border-light mt75  my-5">
			<br><br><br>
			<h1 class="text-center font-italic">
				Contrôle de produits à la réception 
			</h1>
			{{ form_start(form) }}
			{{ form_row(form.date,  {'attr':{'class':'form form-control'},}) }}
			{{ form_row(form.productName,  {'attr':{'class':'form form-control'}}) }}
				{{ form_row(form.temperature,  {'attr':{'class':'form form-control'}}) }}
			{{ form_row(form.etatemballage,  {'attr':{'class':'form form-control'}}) }}
			{{ form_row(form.numerolot,   {'attr':{'class':'form form-control'}}) }}
            {{ form_row(form.dlcdluo,   {'attr':{'class':'form form-control'}}) }}
           
            {{ form_row(form.conformity_etiquetage,   {'attr':{'class':'form form-control'}}) }}
            {{ form_row(form.conformite_reception_yn,   {'attr':{'class':'form form-control'}}) }}
            {{ form_row(form.observations,   {'attr':{'class':'form form-control'}}) }}
            {{ form_row(form.numero_bliv,   {'attr':{'class':'form form-control'}}) }}
            {{ form_row(form.visalivreur,   {'attr':{'class':'form form-control'}}) }}
            {{ form_row(form.visareceptionnaire,   {'attr':{'class':'form form-control'}}) }}
            
			{{ form_end(form) }}

		</div>
</div>
{% endblock %}
